<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="NaviButtonView">
        <attr name="colorSel" format="color" />
        <attr name="colorUnSel" format="color" />
        <attr name="textArray" format="reference|integer" />
        <attr name="imagesSelArray" format="reference|integer" />
        <attr name="imagesUnSelArray" format="reference|integer" />
        <attr name="count" format="integer" />
    </declare-styleable>
    <declare-styleable name="MTopBarView">
        <attr name="leftImage" format="reference" />
        <attr name="rightImage" format="reference" />
        <attr name="leftText" format="string" />
        <attr name="centerText" format="string" />
        <attr name="rightText" format="string" />
    </declare-styleable>

    <declare-styleable name="RedView">
        <attr name="type" format="integer" />
    </declare-styleable>
    <declare-styleable name="UnderlineTextView">
        <attr name="underlinecolor" format="color" />
    </declare-styleable>

    <declare-styleable name="BoderTextView">
        <attr name="bunderlinecolor" format="color" />
    </declare-styleable>

</resources>
